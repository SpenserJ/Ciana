{
  "panels": {
    "time": {
      "title": "Time",
      "provider": "time",
      "template": "text",
      "size": "r1 c1",
      "settings": {
        "format": "h:mm:ss A"
      }
    },
    "date": {
      "title": "Today's Date",
      "provider": "time",
      "template": "text",
      "toTemplate": "text_date",
      "size": "r1 c1",
      "settings": {
        "format": "LL"
      }
    },
    "rss_twitter_spenserj": {
      "title": "Twitter - @SpenserJ",
      "provider": "rss",
      "template": "html",
      "size": "r4 c3",
      "settings": {
        "frequency": 15,
        "url": "https://api.twitter.com/1/statuses/user_timeline.rss?screen_name=SpenserJ"
      }
    },
    "rss_hacker_news": {
      "title": "Hacker News",
      "provider": "rss",
      "template": "html",
      "size": "r3 c2",
      "settings": {
        "frequency": 15,
        "url": "https://news.ycombinator.com/rss"
      }
    },
    "github_spenserj": {
      "title": "GitHub - SpenserJ",
      "provider": "github/base",
      "template": "text",
      "size": "r2 c2",
      "settings": {
        "api": ["users/%s/events/public", "SpenserJ"],
        "filter": [{ "type": "PushEvent" }]
      }
    }
  }
}
